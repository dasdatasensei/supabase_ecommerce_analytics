# Database Credentials
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_secure_password
DB_NAME=ecommerce-db
DB_SCHEMA=olist

# Supabase Configuration
SUPABASE_URL=http://localhost:8000
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Metabase Configuration
METABASE_DB_NAME=metabase
METABASE_SECRET_KEY=your_metabase_secret_key
METABASE_USER=admin@example.com
METABASE_PASSWORD=your_metabase_password

# Airflow Configuration
AIRFLOW_UID=50000
AIRFLOW_GID=50000
AIRFLOW_FERNET_KEY=your_fernet_key_here
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__WEBSERVER__SECRET_KEY=your_airflow_secret_key
AIRFLOW_ADMIN_USER=admin
AIRFLOW_ADMIN_FIRSTNAME=Admin
AIRFLOW_ADMIN_LASTNAME=User
AIRFLOW_ADMIN_EMAIL=admin@example.com
AIRFLOW_ADMIN_PASSWORD=your_secure_admin_password

# Project Paths
PROJECT_ROOT=/path/to/your/project
DBT_PROFILES_DIR=/path/to/your/project/src/dbt_project/profiles

# Logging
LOG_LEVEL=INFO
LOG_PATH=/path/to/your/project/logs

# Docker Compose Settings
COMPOSE_PROJECT_NAME=supabase-ecommerce